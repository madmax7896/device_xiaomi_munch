# Touchscreen sysfs
genfscon sysfs /devices/platform/soc/990000.spi/spi_master/spi1/spi1.0                    u:object_r:vendor_sysfs_touch:s0
genfscon sysfs /devices/platform/soc/a94000.i2c/i2c-3/3-0049                              u:object_r:vendor_sysfs_touch:s0
genfscon sysfs /devices/platform/soc/a94000.i2c/i2c-4/4-0038                              u:object_r:vendor_sysfs_touch:s0

# Touchpanel sysfs for dt2w
genfscon sysfs /touchpanel                                                                u:object_r:sysfs_touchpanel:s0
